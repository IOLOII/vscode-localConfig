    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    // sync同步使用 自动忽略
    "vscode_vibrancy.opacity": 0.5,
    "vscode_vibrancy.theme": "Default Dark",
    "alias-skip.mappings": {
        "@": "/src" // 默认只有`@`映射，映射到`/src`，你可以添加更多映射，映射路径必须以`/`开头
        ,"./": "/src"
    },
    // eslint主动保存的时候校验并修复
    "eslint.format.enable": true,
